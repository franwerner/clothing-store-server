{
  "compilerOptions": {
    "target": "ESNext",                    
    "module": "ESNext",                  
    "moduleResolution": "node",             
    "sourceMap": true,                     
    "strict": true,                       
    "esModuleInterop": true,              
    "noImplicitAny": true,                
    "outDir": "./dist",                    
    "rootDir": "./src",
    "forceConsistentCasingInFileNames" : true
  },
  "include": [
    "./src/**/*",
    "./global.d.ts"                          
  ],
  "exclude": [
    "node_modules",
    "./ngrok.exe"
  ]
}